// trigger:".logo-team-mercedes",
// scrub: 1,

// ease: "power2"
// },
// scale:0.5, duration:3,


// })


gsap.from(".mercedes-f1",{
scrollTrigger:{
trigger:".mercedes-f1",
scrub: 1,
markers: true,
// end: '50%',
snap: {
snapTo: "labels", // snap to the closest label in the timeline
duration: {min: 0.2, max: 3}, // the snap animation should be at least 0.2 seconds, but no more than 3 seconds
(determined by velocity)
delay: 0.2, // wait 0.2 seconds from the last scroll event before doing the snapping
ease: "power1.inOut" // the ease of the snap animation ("power3" by default)
}
},
x:-500, duration:2

})


